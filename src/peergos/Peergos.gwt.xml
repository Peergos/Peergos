<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0-rc1//EN"
  "http://gwtproject.org/doctype/2.8.0-rc1/gwt-module.dtd">
<module rename-to='peergoslib'>
  <entry-point class='peergos.client.Start'/>
  <public path="gwt/resources"/>
  <super-source path="gwt/emu" />
  <source path="gwt">
    <exclude name="**/emu/**" />
  </source>
  <source path='client'/>
  <source path='shared'/>
  	<add-linker name="sso" />
</module>
